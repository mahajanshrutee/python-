# String slicing
string1 = "Hello World"
print(string1[5:10])
print(string1[0:5])
print(string1[7:])
print(string1[:5])
print(string1[:])
print(string1[-8:-3])
print(string1[5::2])
print(string1[4:9])
print(string1[5:10].upper())
print(string1[5:8].lower())

# String casting
number_str = "123"
n = int(number_str) # Casting string to integer
print(n)
number_int = 5
n = float(number_int)
print(n)
number_str = "59"
n = int(number_str)
print(n)

# String functions
string1 = "hello world"
print(len(string1))
print(string1.upper())
print(string1.lower())
print(string1.split())
print(string1.title())
print(string1.swapcase())
print(string1.capitalize())
print(string1.casefold())
print(string1.encode())
print(string1.replace("hello", "Goodbye"))
